{
    "$schema": "http://json.schemastore.org/package",
    "name": "@kayahr/cdi",
    "version": "0.3.0",
    "type": "module",
    "description": "Simple generic dependency injection library",
    "keywords": [
        "cdi",
        "dependency-injection",
        "context",
        "dependency",
        "dependencies",
        "injection",
        "singleton"
    ],
    "license": "MIT",
    "author": "Klaus Reimer <k@ailis.de>",
    "repository": "github:kayahr/cdi",
    "homepage": "https://github.com/kayahr/cdi",
    "bugs": "https://github.com/kayahr/cdi/issues",
    "funding": "https://github.com/kayahr/cdi?sponsor=1",
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "lib/main/",
        "src/main/"
    ],
    "exports": {
        ".": "./lib/main/index.js",
        "./legacy": "./lib/main/legacy-decorators.js"
    },
    "scripts": {
        "clean": "rm -rf lib",
        "build": "run build:*",
        "build:compile": "tsc",
        "test": "run test:*",
        "test:unit": "ntest",
        "test:lint": "oxlint --type-aware --deny-warnings",
        "test:apidoc": "typedoc --emit none",
        "test:spell": "cspell --no-progress --no-summary",
        "apidoc": "typedoc",
        "prepare": "run clean build"
    },
    "devDependencies": {
        "@kayahr/assert": "1.4.0",
        "@kayahr/cspell": "9.3.2-bundle.1",
        "@kayahr/npm-utils": "1.1.1",
        "@kayahr/ntest": "1.3.0",
        "@kayahr/oxlint-config": "1.14.3",
        "@kayahr/typedoc": "0.28.14-bundle.3",
        "@types/node": "25.0.3",
        "reflect-metadata": "0.2.2",
        "typescript": "5.9.3"
    }
}
